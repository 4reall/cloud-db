{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "indent": [
            true,
            "tabs",
            4
        ],
        "semicolon": false,
        "object-literal-sort-keys": [
            true,
            "match-declaration-order"
        ],
        "ordered-imports": false,
        "member-access": [
            true,
            "no-public"
        ],
        "no-empty": false,
        "no-trailing-whitespace": false,
        "quotemark": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                }
            }
        ]
    },
    "rulesDirectory": []
}
